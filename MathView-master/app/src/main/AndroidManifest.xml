<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nishant.mathsample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/toolBarStyle">

        <activity android:name=".dataFetchingActivityMain" >

        </activity>
        <activity android:name=".dataSyncActivity" />
        <activity android:name=".sqliteToMysql" />
        <activity android:name=".testingMain" />
        <activity android:name=".navigationMenu" />
        <activity android:name=".problemProfileActivity" />
        <activity android:name=".problemActivity" />
        <activity android:name=".menuActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".NetworkMonitor">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
    </application>

</manifest>