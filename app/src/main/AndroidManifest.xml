<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nishant.mathsample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/toolBarStyle">
        <activity android:name=".introApp.introActivity"></activity>
        <activity android:name=".myProfileActivity" />
        <activity android:name=".userProfileActivity" />
        <activity android:name=".statics" />
        <activity android:name=".gridActivity" />
        <activity android:name=".initActivity" android:theme="@style/MyTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".loginActivity" />
        <activity android:name=".signUpActivity" />
        <activity
            android:name=".homeActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".dataFetchingActivityMain" />
        <activity android:name=".dataSyncActivity" />
        <activity android:name=".sqliteToMysql" />
        <activity android:name=".navigationMenu" />
        <activity android:name=".problemProfileActivity" />
        <activity android:name=".problemActivity" />
        <activity android:name=".menuActivity" />

        <receiver android:name=".NetworkMonitor">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
    </application>

</manifest>